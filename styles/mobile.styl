$header-height-mobile = 52px
$tabs-height-mobile = 36px
$controls-height-mobile = 170px
$controls-height-mobile-inner = 140px

$loading = url('data:image/png;base64,')

$color-dark = #171B20
$color-dark-md = #2A323D
$color-dark-lt = #262E38
$color-dark-dk = #1B1F24
$color-dark-mdd = #2A323D
$color-dark-accent = #1A1F24

$color-inputs-dark-bg = #222832
$color-inputs-dark-bg-dk = #222832
$color-dark-separator = #232932
$color-dark-dropdown = #364150
$color-dark-unselected = #607391

$body-bg = #fcfeff
$body-color = #4B657F
$name-color = #2a3235
$font-size = 16px

body
	font-family 'Roboto', sans-serif
	font-size $font-size
	line-height 1.6
	background $color-dark
	height 100%
	color $body-color

@keyframes activate-button
	0%
		background-color rgba(255, 255, 255, 0)

	10%
		background-color rgba(255, 255, 255, .075)

	100%
		background-color rgba(255, 255, 255, 0)

@keyframes ngdialog-opening-mobile
	0%
		transform translateY(25%) scale(.45)
		opacity 0

	100%
		transform translateY(0) scale(.45)
		opacity 1


@keyframes ngdialog-closing-mobile
	0%
		transform translateY(0) scale(.45)
		opacity 1

	100%
		transform translateY(-25%) scale(.45)
		opacity 0

.app
	body
		&.mobile
			min-width 100%
			user-select none

			.ngdialog
				top 0 !important

			.ngdialog-content
				top 0 !important
				transform translateX(0) scale(.45)
				animation ngdialog-opening-mobile .5s cubic-bezier(0.550, 0.085, 0.290, 0.905)

			.ngdialog-closing
				.ngdialog-content
					animation ngdialog-closing-mobile .5s cubic-bezier(0.550, 0.085, 0.290, 0.905)

			&.nav-opened-global
				.ngdialog
					display none

	.mobile-scrollable-content
		height 100%

	//
	//
	//.main-frame-mobile-content-container
	//	height 100%

	.payment-frame-loading
		padding 0 !important
		left 0 !important

	.payment-frame
		width 100% !important
		padding 0 !important
		border none !important
		box-shadow none !important
		margin-bottom 0 !important
		position static !important
		border-radius 0 !important

		iframe
			width 100%
			display block

	.payment-systems-mobile
		padding 10px !important

		.qiwi
			margin-top 20px

		.btn
			height 44px !important
			line-height 44px !important
			min-width 0 !important
			width 100% !important
			padding 0 !important
			margin 0 !important
			font-size 16px !important
			font-weight 600
			box-shadow 0 1px 1px rgba(0, 0, 0, .25)
			border-radius 3px

			&:active
				animation activate-button 1s

		.form.payment-method
			width auto !important
			margin-top 16px !important

			.text-box-input
				box-shadow 0 1px 1px rgba(0, 0, 0, .25)
				border-radius 3px

				.selector
					border none
					border-radius 3px 0 0 3px

				input
					border-radius 0 3px 3px 0

			.inputs
				margin-bottom 0 !important

			.row
				margin-bottom 10px

			.block
				width 50%
				margin 0
				box-sizing border-box

				&:first-child
					padding-right 5px

				&:last-child
					padding-left 5px

		.country-selector
			.flags
				top 52px !important
				width 300px !important
				margin-left -1px

				>span
					width 50px !important
					height 44px !important
					line-height 44px !important

	.payment-settings-mobile
		text-align left

		.form
			&:after
				display: none !important

		.row
			margin-bottom 10px

			.block
				float left
				width 50%
				margin 0
				min-width 0 !important
				box-sizing border-box

				&:first-child
					padding-right 5px

				&:last-child
					padding-left 5px

			&.confirm-bonus
				.form.approve
					height auto !important
					margin-bottom 10px
					text-align center !important
					width auto !important
					float none !important

			&.title
				color #fff
				text-transform uppercase
				font-size 14px
				line-height 20px
				height 20px
				margin-top 20px !important

			&.summary
				border-top 1px solid #575F72
				border-bottom 1px solid #575F72
				line-height 44px

			&.legend
				margin-top 0px !important
				margin-bottom 8px

				.name
					padding-left 10px
					color #939699
					font-size 12px

			.block-status
				width 25%
				float left
				margin 0
				min-width 0 !important
				box-sizing border-box
				text-align left

				&:nth-child(1)
					width 25%

				&:nth-child(2)
					width 26%

				&:nth-child(3)
					width 19%

				&:nth-child(4)
					width 30%

				.summary-title
					color #fff
					text-transform uppercase

				.amount
					color #939699

				.result
					color #1EA7EA !important
					font-weight 600

				.bonus
					color #22C95F !important

				.summary-title,
				.amount,
				.bonus,
				.result
					padding-left 10px
					font-size 16px !important
					vertical-align top
					line-height 44px
					display inline-block
					white-space nowrap
					min-width 0 !important

				.no-bonus
					color #939699 !important

			&.status-list
				.item
					line-height 44px
					height 44px
					overflow hidden
					position relative
					border-radius 3px
					background-color #2C323A
					box-shadow 0 1px 1px rgba(0, 0, 0, .25)
					margin-bottom 10px
					border 1px solid transparent
					transition border-color .2s

					&:before
						content ''
						display block
						top -10px
						left -10px
						width 20px
						height 20px
						position absolute
						box-shadow 0 1px 2px rgba(0, 0, 0, .5)
						transform rotateZ(45deg)
						opacity 0
						transition .2s

					&.active
						&:before
							opacity 1

					&.micro
						border-color RGBA(61, 70, 82, .35)

						&:before
							background-color RGBA(61, 70, 82, .4)

						&.active
							border-color RGBA(61, 70, 82, 1)

							&:before
								background-color RGBA(61, 70, 82, 1)

					&.popular
						border-color RGBA(50, 174, 251, .4)

						&:before
							background-color RGBA(50, 174, 251, .4)

						&.active
							border-color RGBA(50, 174, 251, 1)

							&:before
								background-color RGBA(50, 174, 251, 1)

					&.silver
						border-color RGBA(207, 211, 213, .4)

						&:before
							background-color RGBA(207, 211, 213, .4)

						&.active
							border-color RGBA(207, 211, 213, 1)

							&:before
								background-color RGBA(207, 211, 213, 1)

					&.gold
						border-color RGBA(228, 197, 61, .4)

						&:before
							background-color RGBA(228, 197, 61, .4)

						&.active
							border-color RGBA(228, 197, 61, 1)

							&:before
								background-color RGBA(228, 197, 61, 1)

					&.vip
						border-color RGBA(147, 86, 232, .4)

						&:before
							background-color RGBA(147, 86, 232, .4)

						&.active
							border-color RGBA(147, 86, 232, 1)

							&:before
								background-color RGBA(147, 86, 232, 1)


		.text-box-input
			height 44px
			line-height 44px
			width auto !important
			margin 0 !important
			padding-left 50px !important
			box-shadow 0 1px 1px rgba(0, 0, 0, .25)
			border-radius 3px

			.currencies
				top 54px !important

				>span
					line-height 44px !important
					padding-left 50px !important

					>i
						text-align left !important
						padding-left 13px

			.prefix
				width 50px !important
				height 44px !important
				line-height 44px !important
				top 0 !important
				border-radius 3px 0 0 3px !important

				&:after
					right 8px !important
					font-size 6px !important

				i,
				.dropdown-toggle
					width 50px !important
					height 44px !important
					line-height 44px !important
					font-size 16px !important

					&.dropdown-toggle
						text-align left !important
						padding-left 13px

			input
				height 44px !important
				line-height 44px !important
				border-color $color-inputs-dark-bg-dk !important
				font-size 16px !important
				border-radius 0 3px 3px 0 !important

		.btn
			height 44px !important
			line-height 44px !important
			min-width 0 !important
			width 100% !important
			padding 0 !important
			margin 0 !important
			font-size 16px !important
			font-weight 600
			box-shadow 0 1px 1px rgba(0, 0, 0, .25)
			border-radius 3px

			&:active
				animation activate-button 1s

	.payment-do
		.form.payment-method
			margin-top 0 !important

	.mobile-selector
		background-color: #2C323A
		box-shadow 0 1px 1px rgba(0, 0, 0, .25)
		border-radius 3px
		height 44px
		line-height 44px
		position relative
		margin-bottom 10px

		.current
			&:after
				dropdown-arrow-after()
				color #9FACC6
				right 15px

		.current,
		.item
			padding 0 40px 0 15px
			position: relative

			.icon
				position: absolute
				left 15px
				width 30px
				height 30px
				top 50%
				margin-top -15px
				display: block
				background-repeat no-repeat
				background-position 50% 50%

				&.credit-card
					background-image url('/images/svg/payment-systems/card.svg')

			.title
				color #9FACC6
				white-space nowrap
				overflow hidden
				text-overflow ellipsis

			&.active
				background-color: #2C323A + 10

			&:last-child
				border-radius 0 0 3px 3px

		.items
			position absolute
			z-index 5
			background-color: #2C323A
			box-shadow 0 1px 1px rgba(0, 0, 0, .25)
			border-radius 0 0 3px 3px
			width 100%
			top 100%
			margin-top 1px
			display none
			padding 0

		&.icon
			.current,
			.item
				padding 0 40px 0 60px

		&.opened
			border-radius 3px 3px 0 0
			border-bottom 1px solid #16191D

			.items
				display block

	.main-nav-mobile
		position fixed
		top 0
		left 0
		width 100%
		height 100%
		z-index 1
		transform translateX(-30%)
		opacity 0
		transition .5s

		&.nav-opened
			transform translateX(0)
			opacity 1

		.main-nav-mobile-header
			height $header-height-mobile
			background-color rgba(255, 255, 255, .025)

			.logo
				display block
				width 220px
				height $header-height-mobile
				background url('/images/svg/logo-full-light-blue.svg') no-repeat 15px 50%
				background-size 151px

		.navigation-mobile
			a
				height $header-height-mobile
				line-height $header-height-mobile
				font-weight 300
				display block
				padding 0 20px
				color #4F5667
				background-color transparent
				border-bottom 1px solid rgba(0, 0, 0, .3)

				&.active,
				&:active
					color #109ED9
					animation activate-button 1s

	.main-frame-mobile
		position fixed
		top 0
		left 0
		width 100%
		height 100%
		overflow auto
		z-index 2
		box-sizing border-box
		background-color #21242B
		transition transform .5s

		.main-frame-mobile-container
			transition .5s
			height 100%

		&.viewport-inactive-mobile
			.mobile-scrollable-content,
			.main-frame-mobile-content-container
				$blur-radius = 6px
				pointer-events none

				-webkit-filter blur($blur-radius)
				-moz-filter blur($blur-radius)
				-o-filter blur($blur-radius)
				-ms-filter blur($blur-radius)
				filter blur($blur-radius)


		&.no-sub-nav
			padding-top $header-height-mobile

		&.sub-nav-top
			padding-top $tabs-height-mobile + $header-height-mobile

		&.sub-nav-bottom
			padding-top $header-height-mobile
			padding-bottom $tabs-height-mobile

			.main-frame-mobile-container
				height 100%
				position relative
				overflow auto

		&.nav-opened
			transform translateX(83%)

			.main-frame-mobile-container
				box-shadow 0 5px 8px rgba(0, 0, 0, .15)

			.main-frame-mobile-content-container
				pointer-events none


	.header-mobile
		height $header-height-mobile
		background-color #2A2F38
		box-shadow 0 5px 8px rgba(0, 0, 0, .15)
		position fixed
		z-index 30
		left 0
		top 0
		width 100%

		.left
			position absolute
			left 0
			top 0
			height $header-height-mobile

			.nav-toggler
				display inline-block
				height $header-height-mobile
				vertical-align bottom
				width $header-height-mobile
				position relative
				transition .2s

				i
					width ($header-height-mobile - 30)
					left 50%
					height 3px
					position absolute
					background-color #fff
					margin-left -(($header-height-mobile - 30) / 2)

					&:nth-child(1)
						top 17px

					&:nth-child(2)
						top 24px

					&:nth-child(3)
						top 31px

				&:active
					background-color rgba(255, 255, 255, .2)

		.right
			position absolute
			right 0
			top 0
			height $header-height-mobile

			.item
				display inline-block
				height $header-height-mobile

			.balance
				display inline-block
				height $header-height-mobile
				line-height $header-height-mobile
				margin 0 15px
				color #fff
				vertical-align bottom
				font-size 16px

				&.demo
					position relative

					+below(360px)
						line-height $header-height-mobile + 16

					&:after
						content attr(data-badge-title)
						display inline-block
						text-align center
						height 18px
						margin-left 7px
						line-height 19px
						font-size 10px
						text-transform uppercase
						padding 0 5px
						background-color #d83f3f
						color #fff
						border-radius 4px
						vertical-align middle
						font-weight 500
						position relative
						box-sizing border-box
						top -2px

						+below(360px)
							position absolute
							top 9px
							right 0
							font-size 9px
							line-height 16px
							height 15px

			.deposit,
			.real
				background-color #56B386
				display inline-block
				height $header-height-mobile
				color #fff
				font-style normal
				padding 0 10px
				box-sizing border-box
				font-size 16px
				text-align center
				line-height 1
				position relative
				vertical-align bottom
				text-shadow 0 1px 1px rgba(0, 0, 0, .25)
				font-weight 300

				&:after
					content ''
					display block
					position absolute
					height 3px
					width 100%
					bottom 0
					left 0
					z-index 2
					background-color rgba(0, 0, 0, .125)

				&.real
					font-size 12px
					padding-top 7px

					i
						font-style normal
						font-size 16px

				&.deposit
					padding-top 16px
					font-weight 400

					i
						margin-right 6px

				&.disabled
					opacity .5
					pointer-events none


	.tabs-mobile
		height $tabs-height-mobile
		line-height $tabs-height-mobile
		background-color #262B33
		z-index 20
		position fixed
		left 0
		width 100%

		&.top
			top $header-height-mobile
			box-shadow 0 5px 8px rgba(0, 0, 0, .25)

		&.bottom
			bottom 0
			box-shadow 0 -5px 8px rgba(0, 0, 0, .25)

		.item
			float left
			text-align center
			font-size 12px
			text-transform uppercase
			color #4F5666
			transition .2s
			position relative

			&:after
				content ''
				display block
				position absolute
				bottom 0
				left 50%
				height 1px
				width 0
				margin-left 0
				background-color #109ED9
				transition .2s
				opacity 0

			&.active
				color #109ED9
				animation activate-button 1s

				&:after
					opacity 1
					width 100%
					margin-left -50%

		&.two
			.item
				width 50%

		&.three
			.item
				width 33.33333333333%

	.traderoom-mobile-viewport
		overflow hidden
		width 100%

		.canvas-wrapper
			padding 0 !important

			.loading
				padding 0

				.container
					transform scale(.715)

		.traderoom-viewport-panes
			position relative
			transition .5s

			.pane
				float left

				&.scrolling
					overflow auto

		.notify
			text-align center
			padding 20px

		.controls
			background-color #2A2F38
			height $controls-height-mobile
			position relative
			box-shadow 0 -3px 6px rgba(0, 0, 0, .2)

			.left
				position absolute
				width 50%
				height 100%
				left 0
				top 0
				box-sizing border-box
				padding 15px 8px 15px 15px

			.right
				position absolute
				width 50%
				height 100%
				right 0
				top 0
				box-sizing border-box
				padding 15px 15px 15px 8px

			.deal-settings
				border-radius 2px
				background-color #333842
				box-shadow 0 1px 2px rgba(0, 0, 0, .25)
				height $controls-height-mobile-inner

				.block
					color #586173
					height ($controls-height-mobile-inner / 3)px
					border-bottom 1px solid #2A2E36
					position relative
					width 100%
					padding-left 50px
					box-sizing border-box
					text-shadow 0 1px 1px rgba(0, 0, 0, .2)
					line-height 1
					transition .2s

					&:last-child
						border-bottom none

					input
						position absolute
						top 0
						left 0
						height 100%
						width 100%
						padding 0
						display block
						border none
						z-index 2
						opacity 0
						box-sizing border-box
						padding-top 12px
						padding-left 13px
						font-size 16px
						font-family 'Roboto'

						&.prefix
							padding-left 22px

					&:first-child
						border-radius 2px 2px 0 0

					&.interactive
						&:active
							background-color rgba(255, 255, 255, .05)

					.title
						font-size 11px
						text-transform uppercase
						position absolute
						left 13px
						top 6px

					.amount
						color #fff
						position absolute
						left 13px
						top 22px

					.icon-font-keyboard-icon
						display block
						position absolute
						line-height ($controls-height-mobile-inner / 3)
						text-align center
						height ($controls-height-mobile-inner / 3)
						width ($controls-height-mobile-inner / 3)
						right 0
						top 0

					&.earning
						padding 0

						.title
							position absolute
							left 0
							width 100%
							text-align center

						.amount
							color #3BB457
							position absolute
							left 0
							width 100%
							text-align center

			.operate-button
				height ($controls-height-mobile-inner / 2) - 8
				line-height ($controls-height-mobile-inner / 2) - 8
				color #fff
				font-size 22px
				display block
				text-align center
				transition .15s
				padding-right 5px
				text-transform uppercase
				cursor pointer
				border-radius 2px
				box-shadow 0 1px 2px rgba(0, 0, 0, .25)
				text-shadow 0 1px 1px rgba(0, 0, 0, .2)

				&.disabled
					opacity .5
					cursor default
					pointer-events none

				i
					margin-right 10px
					font-size 20px

				&.call
					background #49C574
					margin-bottom 16px

					&:hover
						background #49C574 - 15

				&.put
					background #EB5153

					&:hover
						background #EB5153 - 15

			.new-button
				border-radius 5px
				color #0D90CF
				display block
				text-align center
				transition .15s
				font-weight 300
				text-transform uppercase
				cursor pointer
				height auto
				line-height 1.3
				padding 10px 10px
				font-size 16px
				box-sizing border-box
				border 1px solid #0D90CF

				&:hover
					border-color #0D90CF + 20
					color #0D90CF + 20

				&:active
					background-color RGBA(16, 151, 213, .05)

		.table
			table
				width 100%

				tr
					cursor pointer

					&:nth-child(2n)
						background #F4F9FC

				td
					font-size 14px
					vertical-align middle
					text-align left
					padding 0 8px
					height 70px
					line-height 1.25em
					color #6F7C8E
					border-bottom 1px solid #E7EBED
					white-space nowrap

					.pie-chart
						$size = 32px

						width $size
						height $size
						transition .3s
						opacity 0
						transform scale(0)

						&.ready
							opacity 1
							transform scale(1)

						.value
							font-size 14px
							line-height $size
							width $size
							height $size
							display block
							z-index 2
							top 0
							left 0
							position absolute
							text-align center

					&.unaccent
						span
							font-size 14px
							font-weight normal

					.put-call
						i
							overflow hidden
							height 6px
							display inline-block
							margin-left 3px
							font-size 16px

						&.put
							i
								color #E16C6D
								transform rotateZ(180deg)

						&.call
							i
								color #49C574

					&.small
						font-size 14px

					&.info
						white-space normal

					&.centered
						text-align center

					&:first-child
						padding-left 20px

					&:last-child
						padding-right 20px

					span
						display block
						font-size 12px
						font-weight 300
						color #89A2B4

					.interval
						span
							display inline-block
							margin 0 10px 0 0

							&:last-child
								margin-right 0

	.billing-mobile
		padding 10px

	.user-stats-mobile
		cf()
		margin 15px

		.col
			col(1/3)

